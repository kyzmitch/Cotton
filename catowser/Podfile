# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'catowser' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for catowser
  pod 'SnapKit', '~> 5.0.0'
  pod 'ReactiveSwift', '~> 6.0.0'
  pod 'SwiftLint'
end

target 'CoreBrowser' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CoreBrowser
  pod 'ReactiveSwift', '~> 6.0.0'
  pod 'SwiftLint'
end

target 'JSPlugins' do
  use_frameworks!

  pod 'SwiftSoup'
  pod 'SwiftLint'
  pod 'ReactiveSwift', '~> 6.0.0'
end

target 'HttpKit' do
  use_frameworks!

  pod 'ReactiveSwift', '~> 6.0.0'
  pod 'SwiftLint'
  pod 'SWXMLHash', '~> 5.0.0'
end
